<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

	<table name="food_department" catalog="hotel">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="food_order" catalog="hotel">
		<primary-key>
			<generator class="uuid.string"></generator>
		</primary-key>
	</table>
	<table name="house_order" catalog="hotel">
		<primary-key>
			<generator class="uuid.string"></generator>
		</primary-key>
	</table>
	<table name="leaguer_log" catalog="hotel">
		<primary-key>
			<generator class="uuid.string"></generator>
		</primary-key>
	</table>
	<table name="leaguer" catalog="hotel"></table>
	<table name="house" catalog="hotel">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="vip" catalog="hotel">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
</hibernate-reverse-engineering>